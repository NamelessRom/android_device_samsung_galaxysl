# mount point	fstype		device			options (length for cryptfs encryption footer)
/boot			mtd		boot
/radio			mtd		radio
/cache			ext4	/dev/block/mmcblk0p2					flags=backup=1;canbewiped;wipeingui;wipeduringfactoryreset
/system			yaffs2	system		flags=backup=1;canbewiped;wipeingui
/data			ext4	/dev/block/mmcblk0p3	flags=backup=1;canbewiped;wipeingui;wipeduringfactoryreset
/sdcard			auto	/dev/block/mmcblk0p1	flags=display="Internal SD Card";storage;wipeingui;settingsstorage
/external_sd	auto	/dev/block/mmcblk1p1	flags=display="External SD Card";storage;wipeingui;removable