# mount        fstype  device

/boot          mtd     boot
/recovery      mtd     recovery

/cache         ext4    /dev/block/mmcblk0p2
/data          ext4    /dev/block/mmcblk0p3
/system        yaffs2  system

/sdcard        vfat    /dev/block/mmcblk0p1 lun=/sys/devices/platform/omap/musb-omap2430/musb-hdrc/gadget/lun0/file
/external_sd   vfat    /dev/block/mmcblk1p1 lun=/sys/devices/platform/omap/musb-omap2430/musb-hdrc/gadget/lun1/file
